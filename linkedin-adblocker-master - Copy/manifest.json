{
    "manifest_version": 2,
    "name": "LinkedIn utility AdBlocker",
    "description": "Blocking ads.",
    "version": "1.0.0",
    "author": "Aditya Krishna R S",
    "browser_action": {
        "default_title": "LinkedIn utility AdBlocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.linkedin.com/"
    ],
    "background": {
        "scripts": [
            "extension.js"
        ]
    }
}